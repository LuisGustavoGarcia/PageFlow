{
  "name": "PageFlow",
  "version": "1.0",
  "description": "Webpage interaction for the visually impaired.",
  "permissions": ["declarativeContent"],
  "manifest_version": 2,
  "background": {"page": "background.html"},
  "content_scripts": [ {
    "js": [ "jquery-3.3.1.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
